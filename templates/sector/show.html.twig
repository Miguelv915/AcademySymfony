{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Sector' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'sector_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',sector.nombre) }}
    {{ table.tbl_tr('Descripcion',sector.descripcion) }}
    {% include 'crud/_show_info.html.twig' with {'entity': sector} %}
    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'sector','entity': sector} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'sector','entity': sector} %}
{% endblock %}
