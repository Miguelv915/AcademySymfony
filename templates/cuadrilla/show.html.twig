{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import 'theme/action.html.twig' as action %}

{% set _title = 'Cuadrilla' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'cuadrilla_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
        {{ action.tbl_tr('Alias',cuadrilla.alias) }}
        {% include 'crud/_show_info.html.twig' with {'entity': cuadrilla} %}
    {{ action.tbl_end() }}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'cuadrilla','entity': cuadrilla} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'cuadrilla','entity': cuadrilla} %}
{% endblock %}
