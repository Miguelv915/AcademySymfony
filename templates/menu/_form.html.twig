{% import '@App/theme1/form.html.twig' as frm %}

{{ frm.start(form) }}
    {{ frm.column2(
        form_row(form.parent)
    ) }}
    {{ frm.column2(
        form_row(form.name),
        form_row(form.route)
    ) }}
    {{ frm.column2(
        form_row(form.ranking),
        form_row(form.icon)
    ) }}
    {{ frm.column2(
        form_row(form.badge)
    ) }}

    {{ frm.button(button_label|default('Guardar')) }}
{{ frm.end(form) }}
